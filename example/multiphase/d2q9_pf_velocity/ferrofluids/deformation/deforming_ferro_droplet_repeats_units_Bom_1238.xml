
<?xml version="1.0"?>
<CLBConfig version="2.0" output="output/Bom_1238_inaccurate/">
<!-- 
	Model:	d2q9_pf_velocity_ferro
		Can also be run with d2q9_pf_velocity_CM if the collision type is changed.

	This is the test case from Yos et al. (2019) - Journal of Computational Physics
	Density ratio:10
	Viscosity ratio: 6.324555
	Eotvos number = 100
	Reynolds = 10
	t* = 3750

   Created: 	14-08-2017 
    By:		T.Mitchell
-->
<Units>
	<Param value="0.000025m" gauge="1"/>
	<Param value="1m2/s" gauge="1"/>
</Units>
<Geometry nx="5mm" ny="5mm" >
	<MRT><Box /></MRT>
	<Wall mask="ALL">
	    <Box   ny="1"  />
	    <Box   dy="-1" />
	    <Box   nx="1"/>
	    <Box   dx="-1"/>
	</Wall> 
	<North name="North">
	    <Box dy="-1"/>
	</North>
	<South name="South">
	    <Box ny="1"/>
	</South>
	<West name="West">
	    <Box nx="1"/>
	</West>
	<East name="East">
	    <Box dx="-1"/>
	</East>
</Geometry>
<Model>
	<Param name="Density_h" value="1.58e3kg/m3" />    
	<Param name="Density_l" value="0.8e3kg/m3" />    

	<Param name="Viscosity_h" value="16e-3Pas" />    
	<Param name="Viscosity_l" value="0.8e-3Pas" />    

	<Param name="PhaseField_h" value="1" />    
	<Param name="PhaseField_l" value="0" />    
	<Param name="PhaseField_init"   value="1" />    

	<Param name="Radius"   value="0.5mm" />
	<Param name="CenterX"  value="2.5mm" />
	<Param name="CenterY"  value="2.5mm" />
	<Param name="BubbleType"  value="1" />

	<Param name="M"     value="0.10" />   
	<Param name="W"     value="3.0560" /> 
	<Param name="sigma" value="3.07mN/m" />

	<Param name="mu_1" value = "1.000000N/A2" />
	<Param name="mu_2" value = "3.200000N/A2" />
	<Param name="mu_0" value = "1.000000N/A2"/>

	<Param name="Hy_far" value="5.5kA/m"/>
	<Param name="Hx_far" value="0.0000A/m"/>

	<Param name="relaxation" value="0.078"/>

	<Param name="BuoyancyY" value="0.0000" />
</Model>
<VTK />
<Repeat Times="10">
	<Repeat Times="10000"> 
		<Container>
			<Stop DiffBBelow="1e-3kg/s/A/m3" Times="2" Iterations="100"/>
			<RunAction name="MagToSteadyState" Iterations="1000000"/>
		</Container>
		<Solve Iterations="1"/>
	</Repeat>
	<VTK/>
</Repeat>
</CLBConfig>
